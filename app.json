{
  "expo": {
    "name": "Volunteer-App",
    "slug": "Volunteer-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/icon/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "assets/images/icon/Dark.png",
        "light": "assets/images/icon/Light.png",
        "tinted": "assets/images/icon/Tinted.png"
      },
      "bundleIdentifier": "com.gitcuber369rn.VolunteerApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/icon/adaptive-icon.png",
        "monochromeImage": "assets/images/icon/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gitcuber369rn.VolunteerApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "assets/images/icon/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "assets/images/icon/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9c4ef2f6-0625-45c7-b871-ca342b91a1ff"
      }
    }
  }
}
